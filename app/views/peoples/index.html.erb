<%= render "/navbar" %>

<div class="column is-two-fifths">
<p id="notice"><%= notice %></p>

<h1 class="title ">Users</h1>

<table class="table">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.login, people_path(user.id) %></td>
        <td></td>
        <% end %>
      </tr>
  </tbody>
</table>

</div>

